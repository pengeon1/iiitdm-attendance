{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/attendance",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "templates/attendance.html"
    }
  ]
}
